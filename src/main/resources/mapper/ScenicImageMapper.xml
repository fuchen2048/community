<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fuchen.travel.mapper.ScenicImageMapper">

    <!--List<ScenicImage> selectScenicImageByScenicId(@Param("scenicId") Integer scenicId);-->
    <select id="selectScenicImageByScenicId" resultType="ScenicImage">
        select id, scenic_id, image_url
        from scenic_image
        where scenic_id = #{scenicId}
    </select>

    <insert id="">
      
    </insert>
</mapper>